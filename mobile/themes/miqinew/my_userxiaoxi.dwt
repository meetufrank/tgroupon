<script type="text/javascript">

function localink(linkid){
  event.preventDefault();
  layer.open({
              type: 2
              ,content: '加载中'
            ,time: 0.5 //2秒后自动关闭
          });
    $.ajax({
      type:"POST",
      url:"my_userxiaoxi.php",
      dataType: "json",
      data:{id:linkid,act:'loca_link'},
      success: function(data){
         if(data){
           window.location.href=data;
         }
      }
    });
}

          var page=2;
            $("#getValue-2").click(function(){
                event.preventDefault();
                $.ajax({
                    type:"POST",
                    url:"my_userxiaoxi.php",
                    dataType: "json",
                    data:{page:page,act:'get_next_list'},
                    success: function(data){
                                      // console.log(data);return;
            $.each(data.data,function(i){
              layer.open({
                            type: 2
                            ,content: '加载中'
                          ,time: 0.5 //2秒后自动关闭
                        });

                           $("#tab3 br").before('<a href="#" onclick="localink('+data.data[i].xxid+');"><div class="tab-content-mes col-lg-12 col-md-12 col-sm-12 col-xs-12"> <div class="mysystem_messages col-lg-7 col-md-7 col-sm-7 col-xs-12">'+data.data[i].xxtitle+'</div><div class="mysystem_time col-lg-5 col-md-5 col-sm-5 col-xs-12"><span class="mysystem_time-main">'+data.data[i].xxtime+'</span></div></div></a>');//内容添加到页面上.
                             });
                         if(data.more==0){
                                    $("#more").remove();
                         }
                         page++;
                    }
                });

            });


</script>

